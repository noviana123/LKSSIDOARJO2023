    "#Import libary-libary yang digunakan\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "import seaborn as sns"
   ]
  },
  {
   "cell_type": "raw",
   "id": "e1b57b22",
   "metadata": {},
   "source": [
    "#Menload data\n",
    "df = pd.read_csv(\"Nama_Excel.csv\")\n",
    "df"
   ]
  },
  {
   "cell_type": "raw",
   "id": "952e53d6",
   "metadata": {},
   "source": [
    "Penjelasan disetiap arti variabelnya."
